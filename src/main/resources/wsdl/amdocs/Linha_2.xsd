<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="http://www.vivo.com.br/MC/Assinatura" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ass="http://www.vivo.com.br/MC/Assinatura" xmlns:ps="http://www.vivo.com.br/MC/Pessoa" xmlns:cat="http://www.vivo.com.br/MC/Catalogo" xmlns:ct="http://www.vivo.com.br/MC/Conta" xmlns:sg="http://www.vivo.com.br/MC/Sigan" xmlns:gr="http://www.vivo.com.br/MC/Geral" xmlns:Q1="http://www.vivo.com.br/MC/NFO">
  <xs:include schemaLocation="Linha_334.xsd"/>
  <xs:include schemaLocation="Linha_335.xsd"/>
  <xs:include schemaLocation="Linha_336.xsd"/>
  <xs:include schemaLocation="Linha_337.xsd"/>
  <xs:import schemaLocation="Linha_3.xsd" namespace="http://www.vivo.com.br/MC/Pessoa"/>
  <xs:include schemaLocation="Linha_324.xsd"/>
  <xs:import schemaLocation="Linha_4.xsd" namespace="http://www.vivo.com.br/MC/Pessoa"/>
  <xs:import schemaLocation="Linha_30.xsd" namespace="http://www.vivo.com.br/MC/NFO"/>
  <xs:import namespace="http://www.vivo.com.br/MC/SCA" schemaLocation="Linha_5.xsd"/>
  <xs:import namespace="http://www.vivo.com.br/MC/Conta" schemaLocation="Linha_231.xsd"/>
  <xs:import namespace="http://www.vivo.com.br/MC/Catalogo" schemaLocation="Linha_13.xsd"/>
  <xs:import namespace="http://www.vivo.com.br/MC/Pessoa" schemaLocation="Linha_333.xsd"/>
  <xs:include schemaLocation="Linha_11.xsd"/>
  <xs:include schemaLocation="Linha_204.xsd"/>
  <xs:include schemaLocation="Linha_338.xsd"/>
  <xs:include schemaLocation="Linha_341.xsd"/>
  <xs:include schemaLocation="Linha_342.xsd"/>
  <xs:include schemaLocation="Linha_185.xsd"/>
  <xs:include schemaLocation="Linha_292.xsd"/>
  <xs:include schemaLocation="Linha_343.xsd"/>
  <xs:include schemaLocation="Linha_355.xsd"/>
  <xs:include schemaLocation="Linha_356.xsd"/>
  <xs:include schemaLocation="Linha_357.xsd"/>
  <xs:include schemaLocation="Linha_358.xsd"/>
  <xs:include schemaLocation="Linha_359.xsd"/>
  <xs:include schemaLocation="Linha_360.xsd"/>
  <xs:include schemaLocation="Linha_192.xsd"/>
  <xs:include schemaLocation="Linha_361.xsd"/>
  <xs:include schemaLocation="Linha_362.xsd"/>
  <xs:include schemaLocation="Linha_363.xsd"/>
  <xs:include schemaLocation="Linha_364.xsd"/>
  <xs:include schemaLocation="Linha_365.xsd"/>
  <xs:include schemaLocation="Linha_366.xsd"/>
  <xs:include schemaLocation="Linha_367.xsd"/>
  <xs:include schemaLocation="Linha_368.xsd"/>
  <xs:include schemaLocation="Linha_167.xsd"/>
  <xs:import namespace="http://www.vivo.com.br/MC/Sigan" schemaLocation="Linha_38.xsd"/>
  <xs:import namespace="http://www.vivo.com.br/MC/Geral" schemaLocation="Linha_55.xsd"/>
  <xs:import namespace="http://www.vivo.com.br/MC/Geral" schemaLocation="Linha_184.xsd"/>
  <xs:include schemaLocation="Linha_369.xsd"/>
  <xs:complexType name="Linha">
    <xs:annotation>
      <xs:documentation>Mantem informacoes das linhas.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ass:Assinatura">
        <xs:sequence>
          <xs:element name="codigoLinhaControle" nillable="true" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Codigo que identifica a linha no sistema
								NGIN (account_id) apenas usado para
								linhas do plano controle. Campo
								necessario para a segunda fase do
								projeto controle.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:maxLength value="15"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="seqNumeroLinha" nillable="true" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Codigo sequencial usado para identificar
								uma associacao entre o numero e a
								assinatura no Atlys. No vivonet essa
								informacao encontra-se na coluna
								LINHA.LINHABASE.IDNUMEROSISTEMAORIGEM e
								no Atlys e a coluna
								ACCESS_LINE_NBR_ASGM.ACCESS_LINE_SEQ_NBR.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:maxLength value="15"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="numeroLinha" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Numero do telefone do cliente (DDD +
								numero da linha) - Alterado o MaxLength
								de 10 para 21 para atender necessidade
								do SDP (Open Vivo) Numero do telefone do
								cliente pode iniciar pelo codigo do pais</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:maxLength value="21"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="imeiEsn" nillable="true" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Numero do IMEI (de aparelhos GSM) ou do
								ESN (para aparelhos CDMA).</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:maxLength value="17"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="iccid" nillable="true" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Numero do ICCID</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:maxLength value="20"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="statusIccid" type="ass:StatusIccid" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Referencia a entidade StatusIccid</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="divulgacao" nillable="true" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indica se a linha pode ser divulgada em
								uma lista telefonica. False - linha nao
								pode ser divulgada; True - linha pode
								ser divulgada.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:boolean"/>
            </xs:simpleType>
          </xs:element>
          <xs:element name="dataAutorizacaoDivulgacao" nillable="true" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Data em que o cliente autorizou a
								divulgacao da linha em lista telefonica.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:dateTime"/>
            </xs:simpleType>
          </xs:element>
          <xs:element name="avisoSms" type="ass:AvisoSms" minOccurs="0"/>
          <xs:element minOccurs="0" name="saldos" type="ass:Saldos">
            <xs:annotation>
              <xs:documentation>Referencia a entidade Saldo</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="recargas" type="ass:Recargas" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Referencia a entidade Recarga para
								identificar as recargas que o assinante
								realizou.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ofertas" type="cat:Ofertas">
            <xs:annotation>
              <xs:documentation>Referencia a entidade MatrizOferta</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="promocoesLinha" type="ass:PromocoesLinha" minOccurs="0"/>
          <xs:element name="atributosLinha" type="ass:AtributosLinha" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Referencia a entidade AtributosLinha
								para identificar os atributos (IMEI,
								ESN, ICCID, etc) que ja passaram pela
								linha.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="uf" type="cat:Uf" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Referencia a entidade Uf para
								identificar qual eh a Uf associada ao
								DDD da linha.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="possuiOfertaDisponivel" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indica se a linha possui ofertas
								disponiveis.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:boolean"/>
            </xs:simpleType>
          </xs:element>
          <xs:element name="indicaPortOut" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indica se a linha esta em processo de
								portOut. 0 - false - linha nao esta em
								portout; 1 - true - linha esta em
								processo de portout.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:boolean"/>
            </xs:simpleType>
          </xs:element>
          <xs:element name="aparelhosLinha" type="ass:AparelhosLinha" minOccurs="0"/>
          <xs:element name="indicaRetencaoPendente" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Valores possiveis true e false. Indica
								se a linha possui retencao pendente
								(false=nao possui).</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:boolean"/>
            </xs:simpleType>
          </xs:element>
          <xs:element name="dataSolicitacao" minOccurs="0">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>data de solicitacao</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:dateTime"/>
            </xs:simpleType>
          </xs:element>
          <xs:element name="statusVinculo" type="ass:StatusVinculo" minOccurs="0"/>
          <xs:element name="localidade" type="sg:Localidade" minOccurs="0" maxOccurs="1"/>
          <xs:element name="tipoRede" type="gr:TipoRede" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Referencia a entidade Tiporede parar
								identificar o tipo de rede de uma linha.
								Por ex: Rede Fixa, Rede Movel</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="palavraSeguranca" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Elemento que irá armazenar a palavra
								chave da linha</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="operacoes" type="Q1:Operacoes" minOccurs="0" maxOccurs="1"/>
          <xs:element name="programaRelacionamento" type="ps:ProgramaRelacionamento" minOccurs="0" maxOccurs="1"/>
          <!--Elemento criado por Dilmara Matos em 02/09/2011-->
          <xs:element name="linhasBloqueio" type="ass:LinhasBloqueio" minOccurs="0" maxOccurs="1"/>
          <!--Elemento criado por Dilmara Matos em 02/09/2011-->
          <xs:element name="codigoSigilo" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>elemento que irá identificar o codigo do
								sigilo ex:1,2,3,4 onde 1= Não tem sigilo
								2= Sigilo de Terminal 3= Sigilo de Dados
								Pessoas 4= Sigilo de Terminal e de dados
								pessoais</xs:documentation>
            </xs:annotation>
          </xs:element>
          <!--Elemento criado por Dilmara Matos em 13/09/2011-->
          <xs:element name="motivosEnvioCadastro" type="ps:MotivosEnvioCadastro" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>elemento que referencia a entidade
								MotivosEnvioCadastro</xs:documentation>
            </xs:annotation>
          </xs:element>
          <!--Elemento criado por Dilmara Matos em 13/09/2011-->
          <xs:element name="linhasPublica" type="ass:LinhasPublica" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>elemento que referencia a entidade
								LinhasPublicas</xs:documentation>
            </xs:annotation>
          </xs:element>
          <!--Elemento criado por Dilmara Matos 13/09/2011-->
          <xs:element name="indicadorPortabilidade" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Elemento que irá identificar se há
								portabilidade ex: 0 não e 1 = sim</xs:documentation>
            </xs:annotation>
          </xs:element>
          <!--Elemento criado por Dilmara Matos 15/09/2011-->
          <xs:element name="identificadorLinhaCobranca" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Elemento que irá identificar qual linha
								será utilizada como ponto de cobrança.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <!--Elemento criado por Dilmara Matos 15/09/2011-->
          <xs:element name="categoriaLinha" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Elemento que irá identificar a categoria
								da Linha ex: Residencial (RES), Não
								Residencial (NRES), Tronco(PABX),
								TUP(Orelhão)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dataCadastroPalavraSeguranca" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Data de Cadastro da Palavra de
								Seguranca.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dataAlteracaoPalavraSeguranca" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Data da ultima alteração da Palavra de
								Seguranca.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="indicaFiguracaoListaTelefonica" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indica se a linha esta em figuracao em
								lista telefonica</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dataDesligamento" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Data de desligamento da linha</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dadosIPTV" type="ass:dadosIPTV" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="DadosRedeFTTx" type="ass:DadosRedeFTTx" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="DadosRedeMetalica" type="ass:DadosRedeMetalica" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="conjugado" type="xs:string" minOccurs="0" maxOccurs="1"/>
          <xs:element name="digitoVerificador" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Digito verificador</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ipFixoPlus" type="xs:string" minOccurs="0" maxOccurs="1"/>
          <!--Everis 03/12/12-->
          <xs:element name="classeServicoAtual" type="ass:ClasseServico" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Tipo da linha fixa (somente para linhas
								fixas).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="classeServicoAnterior" type="ass:ClasseServico" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Tipo da linha fixa anterior ao pedido de
								mudança de classe.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="condicaoParticularContratoComercialAtual" type="ass:CondicaoParticularContratoComercial" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Condicoes particulares do contrato
								comercial.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="condicaoParticularContratoComercialAnterior" type="ass:CondicaoParticularContratoComercial" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Condições particulares do contrato
								comercial. Qualificação do terminal
								(tipo linha).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="condicaoTecnicaPrestacaoServico" type="ass:CondicaoTecnicaPrestacaoServico" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Informa a condição técnica de prestação
								de serviço (depende das informações da
								localidade e área de atendimento do
								terminal).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="nomeFiguracaoListaTelefonica" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Nome associado ao terminal na lista
								telefônica.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="enderecosLinha" type="ps:Enderecos" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Enderecos referentes a linha telefonica
								(Ex: linha de instalacao, linha de
								extensao).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="quantidadeLinhasTronco" type="xs:integer" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>QTL é exclusivo de linhas tronco e
								especifica quantas linhas há (em número)
								abaixo da linha principal.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="classeServicoExpansao" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>É a classe do plano de expansão migrado
								para outra localidade, com a classe
								‘Residencial’ ou ‘Negócios’ ("S" ou
								"N").</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="condicaoFiguracao" type="ass:CondicaoFiguracao" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Condição da figuração do assinante na
								lista.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="classificacaoListaTelefonica" type="ass:ClassificacaoListaTelefonica" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Código de identificação do assinante na
								lista telefônica.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="codigoPlataforma" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Codigo do terminal para consulta da
								Plataforma.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="numeroTerminalPlataforma" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Nº de terminal para consulta da
								Plataforma.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="codigoPlataformaCaixaPostal" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Codigo do terminal para consulta da
								Plataforma da Caixa Postal (Somente para
								correspondência).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="numeroTerminalPlataformaCaixaPostal" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Nº de terminal para consulta da
								Plataforma da Caixa Postal (Somente para
								correspondência).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="classeLinhaTroncoChave" type="ass:ClasseServico" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Classe de linha do tronco chave.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dataLimiteLinhaLocacao" type="xs:dateTime" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Data limite de funcionamento da linha no
								caso de uma linha de locação</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="situacaoComercial" type="ass:SituacaoComercial" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Situacao comercial do terminal.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="participacoesFinanceiraAdicional" type="ass:ParticipacoesFinanceiraAdicional" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Participação Financeira Adicional</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="numeroPlanoExpansao" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Nº do Plano de Expansão.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dataLimitePlanoExpansao" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>A mudança de endereço de um terminal
								comprado no plano de expansão só poderá
								ocorrer após a data limite.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="tipoContratoComercial" type="ass:TipoContratoComercial" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Codigo do Tipo do contrato comercial da
								linha. Residencial e não residencial.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dataSolicitacaoLinhaTelefonica" type="xs:dateTime" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Data em que a linha telefônica foi
								solicitada.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="codigoJustificativaTransferenciaAssinatura" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Código da Justificativa da Transferência
								de Assinatura.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="classeServicoAdicional" type="ass:ClasseServico" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Classe do produto no CSO.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="pacote" type="ass:Pacote" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Indicador que a linha pertence a um
								pacote de Bundles.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="numeroEncaminhamentoTecnicoTerminal" type="xs:integer" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Numero de encaminhamento técnico do
								terminal (apenas para terminais
								portados).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="linhaCompartilhada" type="ass:LinhasCompartilhada" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Numero do terminal associado à linha
								compartilhada.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="numeroLinhaSistemaOrigem" type="xs:string" minOccurs="0" maxOccurs="1">
            <xs:annotation>
              <xs:documentation>Terminal CSO com 15 dígitos (5 de
								Localidade + 8 de telefone + 1 de
