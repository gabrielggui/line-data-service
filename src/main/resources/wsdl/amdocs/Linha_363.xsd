  <xs:complexType name="CondicaoFiguracao">
    <xs:annotation>
      <xs:documentation>Condicao da Figuracao do assinante na lista.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="codigo" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Codigo da condicao de figuracao. ex: 0, 1, 4 e
